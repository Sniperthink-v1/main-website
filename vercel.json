{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "aiagents/package.json",
      "use": "@vercel/next",
      "config": { "zeroConfig": true }
    }
  ],
  "routes": [
    {
      "src": "^/aiagents(?:/.*)?$",
      "dest": "/aiagents"
    }
  ]
}
